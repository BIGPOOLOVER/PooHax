local response = req({
    Url = "https://raw.githubusercontent.com/hezzex/scripts/main/Games/" .. game.PlaceId .. ".lua",
    Method = "GET"
})

if response.Body ~= "404: Not Found" then
    loadstring(response.Body)()
else
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "ermmm like what the derp",
        Text = "i dont have a script for this game brah wtf brah",
        Duration = 5
    })

    return
end
