-- yeah im gonna be honest this is all skidded

local response = req({
    Url = "https://raw.githubusercontent.com/hezzexx/scripts/main/Games/" .. game.PlaceId .. ".lua",
    Method = "GET"
})

if response.Body ~= "404: Not Found" then
    game:GetService("Players").LocalPlayer.Idled:Connect(function()
        game:GetService("VirtualInputManager"):SendMouseButtonEvent(0, 0, 2, true, nil, 0)
        wait(1)
        game:GetService("VirtualInputManager"):SendMouseButtonEvent(0, 0, 2, false, nil, 0)
    end)

    
    loadstring(response.Body)()
else
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "brah wtf brah",
        Text = "aw nah brah wtf brah we dont have script for this brah",
        Duration = 5
    })

    return
end
